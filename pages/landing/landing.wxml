<view class="container">
   <view class="banner ">
       <!-- <view class="weatherbox"> </view> -->
       <image class="" src=""/>
   </view>
</view>

<view class="search-bar">
   <image src="../../images/tabbar/search.png" />
   <view id="space"></view>
   <input type="text" name="search" placeholder="Search" class="search-text" bindinput="hideForm"></input>
</view>

<view class="row-between w100 wrap px box-b">
      <view class="photo-box w50 noshrink pa box-b " wx:for="{{events}}" wx:for-item="event">
         <!-- <view class=""> -->
            <image mode="aspectFill" class="w100 radius10" bindtap="goToShow" data-id="{{event.id}}" src="{{event.image_link}}"/>
         <!-- </view> -->
         <text class="title1">{{event.title}}</text>
      </view>
      <view id="line"></view>
</view>

